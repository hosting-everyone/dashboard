      .upgrade-modal .braintree-option__label,
      .upgrade-modal .braintree-toggle,
      .upgrade-modal .paypal-button-tagline,
      .upgrade-modal .braintree-form__label,
      .upgrade-modal .braintree-sheet__text {
        color: white !important
      }
  
      .upgrade-modal .braintree-loader__container,
      .upgrade-modal .braintree-upper-container:before,
      .upgrade-modal .braintree-loader__indicator {
        border: none;
        border-radius: 7px
      }
  
      .upgrade-modal .braintree-sheet,
      .upgrade-modal .braintree-form__hosted-field {
        border: 1px solid #0e1015 !important
      }
  
      .upgrade-modal .braintree-sheet__header {
        border-bottom: 1px solid #0e1015
      }
  
      .upgrade-modal .braintree-form__field {
        background: #272b34;
        color: white
      }
  
      .upgrade-modal .braintree-sheet__label {
        font-size: 18px
      }
  
      .upgrade-modal button {
        margin-top: 1em
      }
  
      .upgrade-overlay>.upgrade-modal>svg,
      .upgrade-modal>svg {
        fill: white
      }
  
      .premium-description {
        font-weight: 300;
        max-width: 60%;
        margin: 1em auto 1.5em;
        text-align: center
      }
  
      .recurring-toggle {
        width: 100%;
        display: flex;
        justify-content: center
      }
  
      .recurring-toggle input[type=checkbox] {
        display: none
      }
  
      .recurring-toggle input[type=checkbox]+label:before {
        background: url(/images/dash/switch-v5-off.svg);
        display: inline-block;
        width: 50px;
        height: 26px;
        content: " ";
        vertical-align: middle
      }
  
      .recurring-toggle input[type=checkbox]:checked+label:before {
        background: url(/images/dash/switch-v5-on.svg);
        display: inline-block;
        width: 50px;
        height: 26px;
        content: " ";
        vertical-align: middle
      }
  
      .recurring-toggle span:first-of-type {
        margin-right: 1em
      }
  
      .recurring-toggle span:last-of-type {
        margin-left: 1em
      }
  
      .recurring-toggle span {
        margin-top: auto;
        margin-bottom: auto
      }
