<script>
  const ToastSubmit = Swal.mixin({
    toast: true,
    position: 'top-right',
    showConfirmButton: false,
    timer: 5000,
    timerProgressBar: true
  })

  $("#bug").submit(function(e) {
    e.preventDefault();
    var form = $(this);

    $.ajax({
      type: "POST",
      url: '/dashboard/<%= req.params.guildID %>/bug',
      data: form.serialize(),
      beforeSend: function() {
        ToastSubmit.fire({
          icon: 'warning',
          title: 'Saving...'
        });
      },
      success: function(data) {
        ToastSubmit.fire({
          icon: 'success',
          title: 'Bug successfully reported!'
        });
      }
    }).fail(function() {
      ToastSubmit.fire({
        icon: 'error',
        title: 'An error occurred while saving',
        text: 'A log was generated and sent to the support server'
      });
    });
  });

  $("#feedback").submit(function(e) {
