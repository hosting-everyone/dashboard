<script>
  function checkTypes() {
    var checkBox = document.getElementById("discordType");
    var div = document.getElementById("discordderver");
    var checkBox1 = document.getElementById("websiteType");
    var div1 = document.getElementById("website");
    var checkBox2 = document.getElementById("dscBotType");
    var div2 = document.getElementById("partnerbot");
    if (checkBox.checked == true) {
      div.style.display = "block";
      div1.style.display = "none";
      div2.style.display = "none";
    } else if (checkBox1.checked == true) {
      div.style.display = "none";
      div1.style.display = "block";
      div2.style.display = "none";
    } else if (checkBox2.checked == true) {
      div.style.display = "none";
      div1.style.display = "none";
      div2.style.display = "block";
    }
  }
</script>

