      const { TwemojiRenderer } = picmoTwemoji;
      document.addEventListener('DOMContentLoaded', () => {
  const selectionContainer = document.querySelector('#selection-outer');
  const emoji = document.querySelector('#selection-emoji');
  // const name = document.querySelector('#selection-name');
  const trigger = document.querySelector('#trigger');
  const selectemoji = document.querySelector('#selectemoji');
  const selectemojiCustom = document.querySelector('#selectemojiCustom');

  const picker = createPopup({
    categories: ['food-drink', 'recents', 'custom', 'smileys-emotion', 'people-body', 'activities', 'animals-nature', 'flags', 'objects', 'symbols', 'travel-places'],
    renderer: new TwemojiRenderer(),
    autoFocus: 'emojis',
    animate: true,
    theme: autoTheme,
    custom: data,
    emojiSize: '1.8rem',
    initialCategory: 'recents',
    showPreview: false,
  }, {
    referenceElement: trigger,
    triggerElement: trigger,
    position: 'right-end'
  });

  trigger.addEventListener('click', () => {
    picker.toggle();
  });

  picker.addEventListener('emoji:select', (selection) => {
    trigger.innerHTML = selection.emoji;
    selectemoji.value = selection.label;
    selectemojiCustom.value = `:${selection.emoji}:${selection.label}`;
    // name.textContent = selection.label;

    // selectionContainer.classList.remove('empty');
  });
});
