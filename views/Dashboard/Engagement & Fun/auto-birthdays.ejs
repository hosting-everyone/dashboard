                        <% if(settings.ModerationClearReplyToggle == true) {%>
                        <input name="dm" id="dm" type="checkbox" class="form-check-input" checked> Send Direct Message
                      </label>
                      <% } else { %>
                      <input name="dm" id="dm" type="checkbox" class="form-check-input"> Send Direct Message
                      </label>
                      <% } %>
                    </div>
                  </div>
                </div>
              </div>
