        <script>
          const Toast = Swal.mixin({
            toast: true,
            position: 'top-right',
            showConfirmButton: false,
            timer: 10000,
            timerProgressBar: true
          })

          // obtem os dados do servidor sobre premium
          var premium = "<%= settings.isPremium.toString() %>";
          // obtem os dados do seletor
          var length = $("#globalchannel").val();
          //var lengthSplit = length.split(", ");
          // verifica se o seletor tem mais de 10 opções e se o servidor é premium
          if (length.length > 10 && premium !== "true") {
            // alert('Oops, você precisa ser premium para adicionar mais de 10 usuarios na whitelist.')
            // limita o seletor a 10
            var le1 = length.splice(1, 10);
            // atualiza os dados do seletor para 10
            $("#globalchannel").val(le1).change();
            // $('#globalchannel').attr('disabled', 'disabled');
          }

          if (length.length == 10) {
            var x = document.getElementById('alert-error');
            if (x.style.display === 'none') {
              x.style.display = 'block';
              $('p#alert-error').html('*You have reached the <code>10</code> channel limit. To exceed this limit, become <a href="/premium" style="padding: 5px;color: #fc424a;font-weight: 300;font-size: 0.875rem;border-radius: 4px;"><code>premium</code></a>.');
              //x.textContent="*You have reached the 10 channel limit. To exceed this limit, become premium."
            } else {
              x.style.display = 'none';
            }
          }

          $("#data1").submit(function(e) {
            e.preventDefault();
            var form = $(this);

            $.ajax({
              type: "POST",
              url: '/dashboard/<%= req.params.guildID %>/automod',
              data: form.serialize(),
              beforeSend: function() {
                Toast.fire({
                  icon: 'warning',
                  title: 'Saving...'
                });
                if (length.length == 10) {
                  var x = document.getElementById('alert-error');
                  if (x.style.display === 'none') {
                    x.style.display = 'block';
                    $('p#alert-error').html('*You have reached the <code>10</code> channel limit. To exceed this limit, become <code>premium</code>.');
                    //x.textContent="*You have reached the 10 channel limit. To exceed this limit, become premium."
                  } else {
                    x.style.display = 'none';
                  }
                }
              },
              success: function(data) {
                Toast.fire({
                  icon: 'success',
                  title: 'Saved Changes'
                });
              }
            }).fail(function() {
              Toast.fire({
                icon: 'error',
                title: 'An error occurred while saving...'
              });
            });
          });
